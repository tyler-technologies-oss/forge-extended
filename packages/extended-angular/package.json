{
  "name": "@tylertech/forge-extended-angular",
  "version": "4.1.0",
  "description": "Forge components adapter library for Angular",
  "private": true,
  "repository": "tyler-technologies-oss/forge-extended",
  "license": "Apache-2.0",
  "publishConfig": {
    "directory": "./dist/forge-extended-angular",
    "linkDirectory": true
  },
  "scripts": {
    "start": "npm run build && ng serve -o",
    "dev": "npm run start",
    "build": "ng build forge-extended-angular --configuration production",
    "build:demo": "ng build --configuration production --stats-json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "postversion": "cd dist/forge-extended-angular && npm version $npm_package_version --no-git-tag-version --no-commit-hooks --ignore-scripts",
    "analyze:bundle": "webpack-bundle-analyzer dist/forge-extended-angular-demo/stats.json",
    "generate-proxies": "ng generate @tylertech/forge-schematics:custom-elements --config generate-proxies.json --interactive=false"
  },
  "dependencies": {
    "@angular/animations": "^17.3.7",
    "@angular/common": "^17.3.7",
    "@angular/compiler": "^17.3.7",
    "@angular/core": "^17.3.7",
    "@angular/forms": "^17.3.7",
    "@angular/platform-browser": "^17.3.7",
    "@angular/platform-browser-dynamic": "^17.3.7",
    "@angular/router": "^17.3.7",
    "@tylertech/forge": "^3.4.0",
    "@tylertech/forge-angular": "^4.1.1",
    "@tylertech/forge-extended": "workspace:*",
    "@tylertech/tyler-icons": "^1.18.1",
    "@tylertech/forge-core": "^3.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.8.1",
    "zone.js": "~0.14.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.6",
    "@angular/cli": "^17.3.6",
    "@angular/compiler-cli": "^17.3.7",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@tylertech-eslint/eslint-plugin": "^2.1.0",
    "@tylertech/forge-schematics": "^1.3.1",
    "@types/jasmine": "~5.1.5",
    "@types/node": "^22.10.2",
    "commitlint": "^19.6.1",
    "husky": "^9.1.7",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "typescript": "~5.4.5"
  }
}
