import { IAccordionAdapter } from './accordion-adapter';
export interface IAccordionCore {
    panelSelector: string;
}
export declare class AccordionCore implements IAccordionCore {
    private _adapter;
    private _panelSelector;
    private _hostInteractionCallback;
    constructor(_adapter: IAccordionAdapter);
    initialize(): void;
    private _attachListeners;
    private _hostInteraction;
    get panelSelector(): string;
    set panelSelector(value: string);
}
