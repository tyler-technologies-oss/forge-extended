  .forge-checkbox:has(input[type=checkbox]:indeterminate) .forge-checkbox__icon::before {
    translate: -100% 0;
  }
}
.forge-checkbox:has(input[type=checkbox]:indeterminate) .forge-checkbox__icon--indeterminate {
  clip-path: none;
