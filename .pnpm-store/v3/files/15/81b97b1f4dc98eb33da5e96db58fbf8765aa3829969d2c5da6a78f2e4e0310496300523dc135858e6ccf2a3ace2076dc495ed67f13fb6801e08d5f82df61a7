export declare const FOCUS_INDICATOR_CONSTANTS: {
    elementName: "forge-focus-indicator";
    attributes: {
        TARGET: string;
        ACTIVE: string;
        INWARD: string;
        CIRCULAR: string;
        ALLOW_FOCUS: string;
        FOCUS_MODE: string;
    };
    defaults: {
        FOCUS_MODE: FocusIndicatorFocusMode;
    };
};
export type FocusIndicatorFocusMode = 'focus' | 'focusin';
