import 'array-flat-polyfill';
import { Reporter } from '@web/test-runner-core';
export interface JUnitReporterArgs {
    outputPath?: string;
    reportLogs?: boolean;
    rootDir?: string;
}
/**
 * A JUnit-format XML file reporter for Web Test Runner
 *
 * @param args Options for JUnit Reporter
 */
export declare function junitReporter({ outputPath, reportLogs, rootDir, }?: JUnitReporterArgs): Reporter;
