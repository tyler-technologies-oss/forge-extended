import { Theme } from '../constants';
export declare const BADGE_CONSTANTS: {
    elementName: "forge-badge";
    attributes: {
        DOT: string;
        HIDE: string;
        THEME: string;
        STRONG: string;
    };
    selectors: {
        ROOT: string;
    };
    classes: {
        OPEN: string;
    };
    defaults: {
        THEME: BadgeTheme;
    };
};
export type BadgeTheme = Theme | 'default' | 'info-primary' | 'info-secondary' | 'danger';
