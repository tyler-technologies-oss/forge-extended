import { ICircularProgressAdapter } from './circular-progress-adapter';
import { CircularProgressTheme } from './circular-progress-constants';
export interface ICircularProgressCore {
    determinate: boolean;
    progress: number;
    theme: CircularProgressTheme;
    track: boolean;
}
export declare class CircularProgressCore implements ICircularProgressCore {
    private _adapter;
    private _determinate;
    private _progress;
    private _theme;
    private _track;
    constructor(_adapter: ICircularProgressAdapter);
    initialize(): void;
    private _applyDeterminate;
    get determinate(): boolean;
    set determinate(value: boolean);
    get progress(): number;
    set progress(value: number);
    get theme(): CircularProgressTheme;
    set theme(value: CircularProgressTheme);
    get track(): boolean;
    set track(value: boolean);
}
