        ALTERNATIVE: string;
        CLUSTERED: string;
        FIXED: string;
        ALIGN_LEFT: string;
        ALIGN_CENTER: string;
        ALIGN_RIGHT: string;
        VERTICAL: string;
        FOCUSED: string;
    };
    selectors: {
        STEPPER: string;
    };
    attributes: {
        SELECTED_INDEX: string;
        LINEAR: string;
        ALTERNATIVE: string;
        LAYOUT_MODE: string;
        LAYOUT_ALIGN: string;
        DISABLED: string;
        VERTICAL: string;
