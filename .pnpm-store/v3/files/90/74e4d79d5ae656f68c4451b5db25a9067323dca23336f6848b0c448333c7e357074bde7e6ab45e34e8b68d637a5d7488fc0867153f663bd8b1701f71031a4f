        SHOW_PROFILE_BUTTON: boolean;
        SIGN_OUT_BUTTON_TEXT: string;
        PROFILE_BUTTON_TEXT: string;
    };
    events: {
        SIGN_OUT: string;
        PROFILE: string;
    };
};
