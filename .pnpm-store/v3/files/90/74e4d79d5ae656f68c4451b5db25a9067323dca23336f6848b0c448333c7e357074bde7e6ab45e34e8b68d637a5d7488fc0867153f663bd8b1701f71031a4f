        EMAIL: string;
        SIGN_OUT: string;
        PROFILE: string;
        SIGN_OUT_TEXT: string;
        PROFILE_TEXT: string;
        AVATAR_TEXT: string;
        AVATAR_ICON: string;
        AVATAR_IMAGE_URL: string;
        AVATAR_LETTER_COUNT: string;
    };
    selectors: {
        FULL_NAME: string;
        EMAIL: string;
        AVATAR: string;
        ACTION_TOOLBAR: string;
        PROFILE_BUTTON: string;
        SIGN_OUT_BUTTON: string;
