import { IAppBarNotificationButtonAdapter } from './app-bar-notification-button-adapter';
export interface IAppBarNotificationButtonCore {
    count: string | number | null | undefined;
    dot: boolean;
    theme: string;
    showBadge: boolean;
    icon: string;
}
export declare class AppBarNotificationButtonCore implements IAppBarNotificationButtonCore {
    private _adapter;
    private _count;
    private _dot;
    private _theme;
    private _showBadge;
    private _isInitialized;
    private _icon;
    constructor(_adapter: IAppBarNotificationButtonAdapter);
    initialize(): void;
    destroy(): void;
    get icon(): string;
    set icon(value: string);
    get count(): string | number | null | undefined;
    set count(value: string | number | null | undefined);
    get dot(): boolean;
    set dot(value: boolean);
    get theme(): string;
    set theme(value: string);
    get showBadge(): boolean;
    set showBadge(value: boolean);
}
