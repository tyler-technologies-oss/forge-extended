import { Theme } from '../constants';
export declare const BUTTON_CONSTANTS: {
    elementName: "forge-button";
    observedAttributes: {
        VARIANT: string;
        PILL: string;
        THEME: string;
        FULL_WIDTH: string;
    };
    attributes: {
        VARIANT: string;
        PILL: string;
        THEME: string;
        FULL_WIDTH: string;
    };
};
