import { IAppBarSearchAdapter } from './app-bar-search-adapter';
export interface IAppBarSearchCore {
    disabled: boolean;
    value: string;
    placeholder: string;
}
export declare class AppBarSearchCore implements IAppBarSearchCore {
    private _adapter;
    private _disabled;
    private _keydownListener;
    constructor(_adapter: IAppBarSearchAdapter);
    initialize(): void;
    private _tryForwardInputState;
    private _onKeydown;
    destroy(): void;
    get disabled(): boolean;
    set disabled(value: boolean);
    get value(): string;
    set value(value: string);
    get placeholder(): string;
    set placeholder(value: string);
}
